100 REM _IOTPUT( "device/leds/trans_ms", R )
110 DEFINTA-Z
120 D$="device/leds/trans_ms":R=1000:GOSUB 1000:END
1000 OUT&H58,&HE0:OUT&H58,1:OUT&H58,&H53
1010 OUT&H58,&HC0:L=LEN(D$)
1020 IF L>=64 THEN LL=127 ELSE LL=L
1030 OUT&H58,LL:LL=LL AND 63:FORI=1TOLL:OUT&H58,ASC(MID$(D$,I,1)):NEXTI
1040 IF L>=64 THEN L=L-63:D$=MID$(D$,64):GOTO 1020
1050 OUT&H58,0:L=INP(&H58)
1060 OUT&H58,&HE0:OUT&H58,1:OUT&H58,&H41
1070 OUT&H58,&HC0:OUT&H58,2:P=VARPTR(R):OUT&H58,PEEK(P):OUT&H58,PEEK(P+1):OUT&H58,0
1080 RETURN
